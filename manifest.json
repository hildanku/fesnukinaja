{
    "manifest_version": 3,
    "name": "FesnukinAja",
    "version": "1.0",
    "permissions": [
        "storage",
        "declarativeNetRequest"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "imphnen.jpg",
            "48": "imphnen.jpg",
            "128": "imphnen.jpg"
        }
    },
    "declarative_net_request": {
        "rule_resources": [
            {
                "id": "block_rules",
                "enabled": true,
                "path": "rules.json"
            }
        ]
    }
}
